# settings
set bell-style none
set completion-ignore-case on
set meta-flag on
set convert-meta off
set output-meta on
set input-meta on
set bind-tty-special-chars off
set blink-matching-paren on

# keybindings
"\ep": previous-history
"\en": next-history
"\C- ": set-mark
"\eOD": backward-word
"\eOC": forward-word
"\e[1;5D": backward-word
"\e[1;5C": forward-word
"\e[1;9D": backward-word
"\e[1;9C": forward-word
