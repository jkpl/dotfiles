loadplugins '\.(js|penta)$'
group user
colorscheme molokai
hi FontCode font-size: 8pt; font-family: "Terminus", "Consolas", "Dejavu Sans Mono", "Courier New";
hi FontFixed font-family: "Terminus", "Consolas", "Dejavu Sans Mono", "Courier New"; font-size: 8pt;
hi FontProportional font-size: 8pt; font-family: "Terminus", "Consolas", "Dejavu Sans Mono", "Courier New";
hi -a Hint font-size: 16px; font-weight: bold; text-transform: uppercase;
map -count -modes=n k -builtin <count><count><C-u>
map -count -modes=n j -builtin <count><count><C-d>
map -count -modes=n K -builtin <count><count>:tabnext<CR>
map -count -modes=n J -builtin <count><count>:tabprevious<CR>
noremap <C-q> :stop<CR>
inoremap <C-j> <CR>
inoremap <C-n> <Down>
inoremap <C-p> <Up>
inoremap <C-f> <Right>
inoremap <C-b> <Left>
inoremap <C-g> <ESC>
cnoremap <C-g> <ESC>
vnoremap <C-g> <ESC>
noremap <C-g> <ESC>
noremap <C-s> /
noremap <C-r> ?
cmap <C-n> <Tab>
cmap <C-p> <S-Tab>
cmap <C-f> <Right>
cmap <C-b> <Left>
cmap <C-j> <CR>
set autocomplete=
set defsearch=s
set fbwhitelist=youtube.com,github.com,soundcloud.com,facebook.com,infoq.com
